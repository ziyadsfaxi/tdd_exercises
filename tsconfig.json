{
    "compilerOptions": {
        "target": "es2016",
        "module": "CommonJS",
        "types": ["node", "jest"],
        "sourceMap": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "exclude": ["node_modules"],
    "include": ["src"]
}
